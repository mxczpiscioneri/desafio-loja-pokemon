{
  "name": "pokemon-store",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:mxczpiscioneri/desafio-loja-pokemon.git"
  },
  "homepage": "https://mxczpiscioneri.github.io/desafio-loja-pokemon",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test-coverage": "react-scripts test --watchAll=false --coverage --no-cache",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .jsx,.js src",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "axios": "^0.21.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "styled-components": "^5.2.1",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@softboxlab/eslint-config-gandalf-lint-react": "^1.3.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios-mock-adapter": "^1.19.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.22.0",
    "gh-pages": "^3.1.0",
    "husky": "^5.2.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/reportWebVitals.js"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -g ./commitlint.config.js -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn lint",
      "pre-push": "yarn test-coverage"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
